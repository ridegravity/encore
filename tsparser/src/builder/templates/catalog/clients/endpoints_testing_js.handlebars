import { apiCall } from "encore.dev/internal/codegen/api";
import { registerTestHandler } from "encore.dev/internal/codegen/appinit";

{{#each endpoints}}
export async function {{name}}(params) {
    const handler = (await import({{toJSON (stripExt import_path)}})).{{name}};
	// TODO(fredr): handle stream endpoints
    registerTestHandler({ service: "{{../name}}", name: "{{name}}", raw: {{toJSON raw}}, handler, streamingRequest: false, streamingResponse: false });
    return apiCall("{{../name}}", "{{name}}", params);
}

{{/each}}
